    "# Activate ipympl\n",
    "%matplotlib widget  \n",
    "\n",
    "import matplotlib.pyplot as plt  \n",
    "import numpy as np  \n",
    "\n",
    "x = np.linspace(0, 10, 100)  \n",
    "y = np.sin(x)  \n",
    "\n",
    "fig, ax = plt.subplots()  \n",
    "ax.plot(x, y)  \n",
    "plt.show()\n"
   ]
  }
